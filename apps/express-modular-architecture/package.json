{
    "name": "@penguin/express-modular-architecture",
    "version": "0.0.1",
    "author": "Samayun Miah Chowdhury<samayun.m.chowdhury@gmail.com>",
    "description": "Penguin is an opinionated express framework provides rapid development facility",
    "scripts": {
        "start": "npm run build && node src/index",
        "dev": "nodemon -L src/index",
        "build": "npm run swagger",
        "swagger": "cd src && node swagger.js",
        "lint": "eslint --ignore-path ./.gitignore .",
        "format": "prettier --write \"src/**/*.js\" "
    },
    "keywords": [
        "nodejs-framework",
        "express-framework",
        "framework"
    ],
    "dependencies": {
        "express": "4.17.2",
        "tslib": "^2.0.0",
        "aws-sdk": "^2.987.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express-fileupload": "^1.2.1",
        "jwt-auth-helper": "^1.0.0",
        "mongoose": "^6.0.4",
        "multer": "^1.4.3",
        "multer-s3": "^2.9.0",
        "swagger-autogen": "^2.17.2",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "nodemon": "^2.0.12",
        "husky": "^4.3.7",
        "jest": "^27.4.5",
        "lint-staged": "^12.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,}": [
            "eslint --cache --fix",
            "prettier --write"
        ]
    }
}
