{
  "root": "apps/expressjs-boilerplate",
  "sourceRoot": "apps/expressjs-boilerplate/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/expressjs-boilerplate",
        "main": "apps/expressjs-boilerplate/src/index.ts",
        "tsConfig": "apps/expressjs-boilerplate/tsconfig.app.json",
        "assets": ["apps/expressjs-boilerplate/*.md"]
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "expressjs-boilerplate:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/expressjs-boilerplate/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/expressjs-boilerplate"],
      "options": {
        "jestConfig": "apps/expressjs-boilerplate/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
